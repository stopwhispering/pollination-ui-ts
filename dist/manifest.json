{
	"_version": "1.12.0",

	"sap.app": {
		"id": "pollination.ui",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "Pollination",
		"description": "Plants &amp; Pollination with OpenUI5",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "set-ach"
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {

		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "pollination.ui.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.104.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.m": {},
				"sap.f": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pollination.ui.i18n.i18n"
				}
			}
		},

		"routing": {
			
		},

		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		}
	}
}